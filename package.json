{
  "name": "weibo-archiver",
  "version": "0.1.11",
  "packageManager": "pnpm@8.9.0",
  "description": "weibo-backup",
  "author": "Chilfish",
  "license": "MIT",
  "scripts": {
    "build:preview": "pnpm -F preview build",
    "build:monkey": "pnpm -F monkey build",
    "dev:preview": "pnpm -F preview dev",
    "dev:monkey": "pnpm -F monkey dev",
    "preview": "pnpm -F preview preview",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "1.0.0-beta.24",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.8.4",
    "@vitejs/plugin-vue": "^4.4.0",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.9.0",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.3",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.11",
    "vite-plugin-static-copy": "^0.17.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
